    "esbuild": "^0.14.8",
    "esbuild-runner": "^2.2.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "grammy": "^1.5.4",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "^2.1.2",
    "tap": "^15.1.5",
    "tsup": "^5.11.9",
