{
  "name": "grammy-pseudo-update",
  "description": "grammY plugin: pseudo (manually generated) updates",
  "version": "0.0.0-development",
  "repository": {
    "type": "git",
    "url": "https://github.com/IlyaSemenov/grammy-pseudo-update"
  },
